<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
        <!-- Rotation settings -->
        <add key="RotationConversion" value="3.0" />
        <add key="ZoomConversion" value="1.3" />
        <add key="Inertia" value="0.8" />

        <!-- Explode animation settings -->
        <add key="SWMotionStudyName" value="KinectExplode" />
        <add key="SWMotionStudyLength" value="8.0" />
        <add key="ExplodeAnimationDisplayLength" value="1.0" />

        <!-- Explode gesture settings -->
        <add key="ExplodeThreshold" value="0.3" />
        <add key="CollapseThreshold" value="-0.3" />
    </appSettings>
</configuration>
